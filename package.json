{
  "name": "clinic-saas-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run prisma:push && npm run prisma:deploy && npm run prisma:generate",
    "prisma:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "next-auth": "^4.24.7",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^5.16.0",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "nanoid": "^5.0.7",
    "next": "14.2.4",
    "qrcode": "^1.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "20.11.30",
    "@types/qrcode": "^1.5.5",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prisma": "^5.16.0",
    "tailwindcss": "^3.4.6",
    "tsx": "^4.11.2",
    "typescript": "5.4.5"
  }
}
